{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "sortOrder": {
      "type": "integer"
    },
    "link": {
      "type": "string"
    },
    "slug": {
      "type": "text"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "isFeatured": {
      "type": "boolean"
    },
    "project_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project-category.project-category",
      "inversedBy": "projects"
    }
  }
