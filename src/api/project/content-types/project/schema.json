{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "longTitle": {
      "type": "text",
      "required": false
    },
    "subTitle": {
      "type": "text",
      "required": false
    },
    "description": {
      "type": "text",
      "required": true
    },
    "cardImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "sortOrder": {
      "type": "integer"
    },
    "link": {
      "type": "string"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "isFeatured": {
      "type": "boolean"
    },
    "project_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project-category.project-category",
      "inversedBy": "projects"
    },
    "location": {
      "type": "string"
    },
    "sqareFootage": {
      "type": "string"
    },
    "type": {
      "type": "text"
    },
    "units": {
      "type": "string"
    },
    "yearCompletionDate": {
      "type": "date"
    },
    "carouselGallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "shared.project-details-sections"
      ]
    }
  }
}
