{
  "kind": "collectionType",
  "collectionName": "project_categories",
  "info": {
    "singularName": "project-category",
    "pluralName": "project-categories",
    "displayName": "Project Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "category",
        "status"
      ]
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "categories"
    }
  }
}
