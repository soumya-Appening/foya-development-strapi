{
  "kind": "collectionType",
  "collectionName": "project_categories",
  "info": {
    "singularName": "project-category",
    "pluralName": "project-categories",
    "displayName": "Project Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": ["category", "status"]
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "category"
    }
  }
}
